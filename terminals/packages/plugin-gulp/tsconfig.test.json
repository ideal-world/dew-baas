{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./test_dist",
    "declaration": true,
    "declarationMap": true,
    "target": "ESNext",
    "module": "commonjs",
    "strict": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "./*.ts",
  ]
}
